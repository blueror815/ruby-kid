- if @user_phone.valid?
  $('#phone_tab').html( "#{render(@user_phones)}" );
- else
  $('#user_phone_number_group_of_#{@user_phone.id}').addClass('has-error');
  var phoneNumberGroup = $('#users_user_phone_number_of_#{@user_phone.id}');
  phoneNumberGroup.attr('data-content', "#{@user_phone.errors.full_messages.join('. ')}").popover('show');